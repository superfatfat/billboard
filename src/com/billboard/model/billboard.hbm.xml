<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.billboard.model.BillboardVO" table="billboard" >
        
        <id name="no" column="no">
        	<generator class="identity"></generator>
        </id>
        
        <property name="title" column="title" />
        <property name="announcer" column="announcer" />
        <property name="releaseDate" column="releaseDate" type="java.sql.Date"/>
        <property name="deadlineDate" column="deadlineDate" type="java.sql.Date"/>
        <property name="content" column="content" />
        
    </class>
</hibernate-mapping>

